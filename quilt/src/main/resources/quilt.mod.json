{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${maven_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "contributors": {
        "${mod_author}": "Owner"
      },
      "contact": {
        "homepage": "${mod_homepage}",
        "sources": "${mod_repository}",
        "issues": "${mod_repository}/issues"
      },
      "license": "${mod_license}",
      "icon": "assets/${mod_id}/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "eu.tomger.flowerpower.quilt.FlowerPowerQuilt",
      "client": "eu.tomger.flowerpower.quilt.client.FlowerPowerClientRenderer"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=${quilt_loader_version}"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=${min_quilt_api_version}"
      },
      {
        "id": "minecraft",
        "versions": ">=${minecraft_version}"
      },
      {
        "id": "architectury",
        "versions": ">=${min_architectury_version}"
      }
    ]
  }
}