{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],

  "contact": {
    "source": "${mod_repository}",
    "issues": "${mod_repository}/issues",
    "website": "${mod_homepage}"
  },

  "license": "${mod_license}",
  "icon": "assets/${mod_id}/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "eu.tomger.flowerpower.FlowerPower::initialize"
    ],
    "client": [
      "eu.tomger.flowerpower.fabric.FlowerPowerClientRenderer"
    ]
  },
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${min_architectury_version}"
  }
}
